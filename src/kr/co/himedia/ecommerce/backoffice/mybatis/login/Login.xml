<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.co.himedia.ecommerce.backoffice.mybatis.login.Login">
	
	<select id="loginProc" parameterType="java.lang.String" resultType="java.lang.String">
		SELECT 
			PASSWD
		FROM
			TB_MNG
		WHERE
			ID = #{id}
	</select>
	
</mapper>